<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net46;netstandard2.0</TargetFrameworks>
    <AssemblyName>Microsoft.Bot.Builder.Calling</AssemblyName>
    <RootNamespace>Microsoft.Bot.Builder.Calling</RootNamespace>
    <PackageId>Microsoft.Bot.Builder.Calling</PackageId>
    <Product>Microsoft Bot Builder SDK</Product>
    <Version>3.0.4</Version>
    <Authors>Microsoft</Authors>
    <Description>
      Microsoft Bot Builder extension for Skype Calling API. It provides the required constructs to extend your bot to handle incoming calls from Skype.
    </Description>
    <PackageProjectUrl>https://github.com/Microsoft/BotBuilder</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Microsoft/BotBuilder</RepositoryUrl>
    <PackageIconUrl>http://docs.botframework.com/images/bot_icon.png</PackageIconUrl>
    <RepositoryType>git</RepositoryType>
    <NeutralLanguage>en-US</NeutralLanguage>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <NoWarn>0419;1591;1701;1702;1705;1998</NoWarn>
  </PropertyGroup>
  
  <ItemGroup>
    <Compile Remove="Properties\**\*" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Microsoft.Bot.Builder\Microsoft.Bot.Builder.2017.csproj" />
    <ProjectReference Include="..\Microsoft.Bot.Builder.Autofac\Microsoft.Bot.Builder.Autofac.2017.csproj" />
  </ItemGroup>

</Project>